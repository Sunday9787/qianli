    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/gulp": "^4.0.14",
    "@types/gulp-postcss": "^8.0.4",
    "@types/gulp-replace": "^1.1.0",
    "@types/gulp-sourcemaps": "^0.0.36",
    "@types/gulp-stylus": "^2.7.6",
    "@types/jest": "29.5.1",
    "@types/node": "18.16.12",
