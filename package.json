    "cssnano": "^6.0.1",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "gulp-postcss": "^9.0.1",
    "gulp-replace": "^1.1.4",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-stylus": "^3.0.1",
    "jest": "29.5.0",
    "postcss": "^8.4.31",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.8.8",
    "source-map-support": "^0.5.20",
    "stylelint": "^15.10.3",
    "stylelint-config-prettier": "^9.0.5",
