{
  "extends": ["stylelint-config-prettier", "stylelint-plugin-stylus/recommended", "stylelint-config-recess-order"],
  "plugins": ["stylelint-prettier"],
  "rules": {
    "stylus/at-rule-no-unknown": [
      true,
      {
        "ignoreAtRules": ["tailwind", "apply", "variants", "responsive", "screen"]
      }
    ]
  }
}
